<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="560.0" prefWidth="800.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainMenu">
    <center>
        <VBox prefHeight="410.0" prefWidth="700.0" BorderPane.alignment="CENTER">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="萌泪爆气表" textAlignment="CENTER" wrappingWidth="700.0">
                    <font>
                        <Font size="48.0" />
                    </font>
                </Text>
                <Pane prefHeight="20.0" prefWidth="700.0" />
                <GridPane alignment="CENTER" prefHeight="300.0" prefWidth="700.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="430.0" minWidth="10.0" prefWidth="430.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Button fx:id="calcu" mnemonicParsing="false" onAction="#gotoCalcu1" prefHeight="36.0" prefWidth="230.0" text="计算歌曲爆点" GridPane.halignment="CENTER" GridPane.hgrow="NEVER" GridPane.valignment="CENTER" GridPane.vgrow="NEVER" />
                        <Button mnemonicParsing="false" onAction="#gotoX5Apk1" prefHeight="36.0" prefWidth="230.0" text="从apk中提取文件" GridPane.halignment="CENTER" GridPane.hgrow="NEVER" GridPane.rowIndex="1" GridPane.valignment="CENTER" GridPane.vgrow="NEVER" fx:id="x5apk" />
                        <Button fx:id="bytes" mnemonicParsing="false" onAction="#gotoBytes1" prefHeight="36.0" prefWidth="230.0" text="bytes文件转化xml" GridPane.halignment="CENTER" GridPane.hgrow="NEVER" GridPane.rowIndex="2" GridPane.valignment="CENTER" GridPane.vgrow="NEVER" />
                        <Button fx:id="scan" mnemonicParsing="false" onAction="#showScan" prefHeight="36.0" prefWidth="230.0" text="查看歌曲爆点" GridPane.halignment="CENTER" GridPane.hgrow="NEVER" GridPane.rowIndex="3" GridPane.valignment="CENTER" GridPane.vgrow="NEVER" />
                        <Button mnemonicParsing="false" prefHeight="36.0" prefWidth="230.0" text="谱面伸缩" GridPane.halignment="CENTER" GridPane.hgrow="NEVER" GridPane.rowIndex="4" GridPane.valignment="CENTER" GridPane.vgrow="NEVER" />
                    </children>
                </GridPane>
            </children>
        </VBox>
    </center>
    <left>
        <Pane prefHeight="410.0" prefWidth="50.0" BorderPane.alignment="CENTER_LEFT" />
    </left>
    <right>
        <Pane prefHeight="410.0" prefWidth="50.0" BorderPane.alignment="CENTER_RIGHT" />
    </right>
    <bottom>
        <GridPane prefHeight="90.0" prefWidth="800.0" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="300.0" prefWidth="300.0" />
                <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="300.0" prefWidth="300.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="90.0" prefHeight="90.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Button fx:id="info" mnemonicParsing="false" onAction="#info" prefHeight="36.0" prefWidth="144.0" text="关于萌泪" GridPane.halignment="RIGHT" GridPane.hgrow="NEVER" GridPane.valignment="TOP" GridPane.vgrow="NEVER" />
                <Button fx:id="exit" mnemonicParsing="false" onAction="#exit" prefHeight="36.0" prefWidth="144.0" text="退出" textAlignment="CENTER" GridPane.columnIndex="2" GridPane.halignment="LEFT" GridPane.hgrow="NEVER" GridPane.valignment="TOP" GridPane.vgrow="NEVER" />
            </children>
        </GridPane>
    </bottom>
    <top>
        <Pane prefHeight="60.0" prefWidth="800.0" BorderPane.alignment="CENTER" />
    </top>
</BorderPane>
